#!groovy
// Run docker build

properies([disableConcurrentBuilds()])

pipeline {
	agent {
		lable 'master'
	}
	options {
		buildDiscarder(logRotation(numToKeepStr: '10', artifactNumToKeepStr: '10'))
		timestamp()
	}
	stages {
		stage("Create Docker Image") {
			steps {
				echo " === Start Building Image ==="
				dir ('docker/toolbox') {
					sh 'docker build .'
				}
			}
		}
	}
}